<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>Sakarya Bilişim Mağazası</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
   
    <link id="callCss" rel="stylesheet" href="~/themes/bootshop/bootstrap.min.css" media="screen" />
    <link href="~/themes/css/base.css" rel="stylesheet" media="screen" />
    <!-- Bootstrap style responsive -->
    <link href="~/themes/css/bootstrap-responsive.min.css" rel="stylesheet" />
    <link href="~/themes/css/font-awesome.css" rel="stylesheet" type="text/css">
    <!-- Google-code-prettify -->
    <link href="~/themes/js/google-code-prettify/prettify.css" rel="stylesheet" />
    <!-- fav and touch icons -->
    <link rel="shortcut icon" href="~/themes/images/ico/favicon.ico">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/themes/images/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/themes/images/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/themes/images/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="~/themes/images/ico/apple-touch-icon-57-precomposed.png">

    <!--iletişim için-->
    <link href="~/Content/iletisim/css/style.css" rel="stylesheet" />
    <script src="~/Scripts/İletisimscripts/Scripts/jmpress.min.js"></script>
    <script src="~/Scripts/İletisimscripts/Scripts/jquery-1.7.1.js"></script>
    <script src="~/Scripts/İletisimscripts/Scripts/jquery.jmslideshow.js"></script>
    <script src="~/Scripts/İletisimscripts/Scripts/modernizr.custom.48780.js"></script>

   

    <style type="text/css" id="enject"></style>
</head>
<body>
  
    <div id="header">
        <div class="container">
            <div id="welcomeLine" class="row">
                <div class="span6"> <b>@E_ticaret.Resources.lang.Hosgeldin</b><strong> </strong></div> @*Buraya User yerine Kullanıcının ismi yazılıcak*@
                <div class="span6">
                    <div class="pull-right">
                        @Html.Partial("_CulturePartial")
                        @*<a href="product_summary.html"><span class="btn btn-mini btn-primary"><i class="icon-shopping-cart icon-white"></i> Sepetiniz !</span> </a>*@
                    </div>
                </div>
            </div>
            <!-- Navbar ================================================== -->
            <div id="logoArea" class="navbar">
                <a id="smallScreen" data-target="#topMenu" data-toggle="collapse" class="btn btn-navbar">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <div class="navbar-inner">
                    @*<a class="brand" href=""><img src="~/themes/images/logo.png" alt="Bootsshop" /></a>*@
                    @Html.ActionLink(" ","Index","Home",null,new   {
        style = "background: url('" + Url.Content("~/themes/images/logo.png") + "') no-repeat center right; display:block; height:45px; width:215px;"
    }
)
                    <form class="form-inline navbar-search" method="post" action="products.html">
                        <input id="srchFld" class="srchTxt" type="text" />
                        <select class="srchTxt">
                            <option>@E_ticaret.Resources.lang.Tum</option>
                            <option>BİLGİSAYARLAR</option>
                            <option>TELEFONLAR</option>
                            <option>TELEVİZYONLAR</option>
                            <option>AKSESUARLAR</option>
                            
                        </select>
                        <button type="submit" id="submitButton" class="btn btn-primary">@E_ticaret.Resources.lang.Ara</button>
                    </form>
                    <ul id="topMenu" class="nav pull-right">
                      
                        @*<li>  @Html.ActionLink("Tum Urunlerimiz", "tumurunler", "Home")</li>*@
                        <li>  @Html.ActionLink(E_ticaret.Resources.lang.Tumkampanya, "tumkampanyalar", "Home", new { @style = "font-weight: bold;" })</li>
                        <li>  @Html.ActionLink(E_ticaret.Resources.lang.Hakkimizdaa, "About", "Home", new { @style = "font-weight: bold;" })</li>
                        <li>  @Html.ActionLink(E_ticaret.Resources.lang.iletisimm, "Contact", "Home", new { @style = "font-weight: bold;" })</li>




                        <li class="">

                            @*<a href="#login" role="button" data-toggle="modal" style="padding-right:0"><span class="btn btn-large btn-success">GİRİŞ</span></a>
                            <div id="login" class="modal hide fade in" tabindex="-1" role="dialog" aria-labelledby="login" aria-hidden="false">
                                <div class="modal-header">
                                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                                    <h3>GİRİŞ YAP</h3>
                                </div>
                                <div class="modal-body">
                                    <form class="form-horizontal loginFrm">
                                        <div class="control-group">
                                            <input type="text" id="inputEmail" placeholder="Email">
                                        </div>
                                        <div class="control-group">
                                            <input type="password" id="inputPassword" placeholder="Şifre">
                                        </div>
                                        <div class="control-group">
                                            <label class="checkbox">
                                                <input type="checkbox"> Beni Hatırla
                                            </label>
                                        </div>
                                    </form>
                                    <button type="submit" class="btn btn-success">Giriş</button>*@
                                    @*if(button type=submit)
                                    {   @Html.ActionLink("İletişim", "Contact", "Home")
                                    }*@

                                    @*<button class="btn" data-dismiss="modal" aria-hidden="true">Kapat</button>
                                </div>
                            </div>
                        </li>*@

                            @Html.Partial("_LoginPartial")

                     
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!-- Header End====================================================================== -->
    <div>@RenderBody()</div>
    
    <div id="footerSection">
        <div class="container">
            <div class="row">
                <div class="span3">
                    <h5>@E_ticaret.Resources.lang.Uyelikislem</h5>
                    @*<a href="login.html">HESABINIZ</a>*@
                    @Html.ActionLink(E_ticaret.Resources.lang.Hesabiniz, "", "Home")
                    @*<a href="login.html">KİŞİSEL BİLGİLERİNİZ</a>*@
                    @Html.ActionLink(E_ticaret.Resources.lang.Bilgileriniz, "", "Home")
                    @*<a href="login.html">ADRESLER</a>*@
                    @Html.ActionLink(E_ticaret.Resources.lang.Adresler, "", "Home")
                    @*<a href="login.html">İNDİRİM</a>*@
                    @Html.ActionLink(E_ticaret.Resources.lang.indirimleriniz, "", "Home")
                    @*<a href="login.html">TÜM SİPARİŞLERİNİZ</a>*@
                    @Html.ActionLink(E_ticaret.Resources.lang.Tumsiparis, "", "Home")
                </div>
                <div class="span3">
                    <h5>@E_ticaret.Resources.lang.Bilgilendirme</h5>
                    @*<a href="contact.html">İLETİŞİM</a>*@
                    @Html.ActionLink(E_ticaret.Resources.lang.Urunler, "Index", "Referanslar")
                    @Html.ActionLink(E_ticaret.Resources.lang.Iletisim, "Contact", "Home")
                    @*<a href="register.html">KAYIT OLMA</a>*@
                    @Html.ActionLink(E_ticaret.Resources.lang.uyeoll, "", "Home")
                    @*<a href="legal_notice.html">DUYURULAR</a>*@
                    @Html.ActionLink(E_ticaret.Resources.lang.Duyurular, "", "Home")
                    @*<a href="tac.html">KOŞULLAR</a>*@
                    @Html.ActionLink(E_ticaret.Resources.lang.Kosullar, "", "Home")
                    @*<a href="faq.html">FAQ</a>*@
                    @Html.ActionLink(E_ticaret.Resources.lang.SSS, "sss", "Home")
                </div>
                <div class="span3">
                    <h5>@E_ticaret.Resources.lang.Teklif</h5>
                    @*<a href="#">YENİ ÜRÜNLERİMİZ</a>*@
                    @Html.ActionLink(E_ticaret.Resources.lang.Urunler, "", "Home")
                    @*<a href="#">EN ÇOK SATANLAR</a>*@
                    @Html.ActionLink(E_ticaret.Resources.lang.Encoksatan, "", "Home")
                    @*<a href="special_offer.html">TEKLİFLER</a>*@
                    @Html.ActionLink(E_ticaret.Resources.lang.Teklif, "", "Home")
                    @*<a href="#">ÜRETİCİLER</a>*@
                    @Html.ActionLink(E_ticaret.Resources.lang.Ureticiler, "", "Home")
                    
                </div>
                <div id="socialMedia" class="span3 pull-right">
                    <h5>@E_ticaret.Resources.lang.Sosyal</h5>
                    <a href="#"><img width="60" height="60" src="~/themes/images/facebook.png" title="facebook" alt="facebook" /></a>
                    <a href="#"><img width="60" height="60" src="~/themes/images/twitter.png" title="twitter" alt="twitter" /></a>
                    <a href="#"><img width="60" height="60" src="~/themes/images/youtube.png" title="youtube" alt="youtube" /></a>
                </div>
            </div>
            <p class="pull-right">&copy; Sakarya Bilişim @DateTime.Now.Year</p>
        </div><!-- Container End -->
    </div>
    <!-- Placed at the end of the document so the pages load faster ============================================= -->
    <script src="~/themes/js/jquery.js" type="text/javascript"></script>
    <script src="~/themes/js/bootstrap.min.js" type="text/javascript"></script>
